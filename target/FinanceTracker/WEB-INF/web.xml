<?xml version="1.0" ?>
<web-app >
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>org.example.bimal.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/runservlet2</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SignUpServlet</servlet-name>
    <servlet-class>org.example.bimal.servlets.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignUpServlet</servlet-name> <!-- Must match the servlet-name above -->
    <url-pattern>/runservlet1</url-pattern>
  </servlet-mapping>
<servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>org.example.bimal.servlets.LogoutServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
</servlet-mapping>
  <servlet>
    <servlet-name>ExpServlet</servlet-name>
    <servlet-class>org.example.bimal.servlets.ExpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExpServlet</servlet-name>
    <url-pattern>/runservlet4</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>IncomeServlet</servlet-name>
    <servlet-class>org.example.bimal.servlets.IncomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IncomeServlet</servlet-name>
    <url-pattern>/runservlet3</url-pattern>
  </servlet-mapping>

</web-app>